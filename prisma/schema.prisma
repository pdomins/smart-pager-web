generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Restaurants {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  slug  String?
  menu  String?

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
